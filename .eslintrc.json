{
    "env": {
        "node": true,
        "browser": true
    },
    "rules": {
        "indent": [
            "error",
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "comma-dangle": ["error", "never"]
    },
    "extends": "eslint:recommended"
}
